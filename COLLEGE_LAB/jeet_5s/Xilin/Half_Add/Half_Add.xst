set -tmpdir ./xst/projnav.tmp
set -xsthdpdir ./xst
run
-ifn Half_Add.prj
-ifmt mixed
-ofn Half_Add
-ofmt NGC
-p acr2
-top Half_Add
-opt_mode Speed
-opt_level 1
-iuc NO
-lso Half_Add.lso
-keep_hierarchy YES
-rtlview Yes
-hierarchy_separator /
-bus_delimiter <>
-case maintain
-verilog2001 YES
-fsm_extract YES -fsm_encoding Auto
-safe_implementation No
-mux_extract YES
-resource_sharing YES
-iobuf YES
-pld_mp YES
-pld_xp YES
-wysiwyg NO
-equivalent_register_removal YES
